{
  "compilerOptions": {
    "baseUrl": "",
    "paths": {
      "services/*": [
        "app/services/*"
      ],
      "search/*": [
        "app/search/*"
      ],
      "model/*": [
        "app/model/*"
      ],
      "profile/*": [
        "app/profile/*"
      ],
      "controls/*": [
        "app/controls/*"
      ],
      "utils/*": [
        "app/utils/*"
      ],
      "@modules/*": [
        "app/modules/*"
      ]
    },
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "../node_modules/@types"
    ]
  }
}
